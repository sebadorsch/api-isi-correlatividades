generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  name      String?
  createdAt DateTime @default(now())
}

enum TermType {
  ANUAL
  CUATRIMESTRAL
}

model Subject {
  id         Int      @id @default(autoincrement())
  name       String   @db.VarChar(100)
  termType   TermType
  totalHours Int
}
